{
  "name": "@pulsebear/speed-insights",
  "version": "0.1.0",
  "description": "Universal Speed Insights SDK for PulseBear",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./astro": {
      "types": "./dist/astro.d.ts",
      "import": "./dist/astro.mjs",
      "require": "./dist/astro.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    },
    "./core": {
      "types": "./dist/core.d.ts",
      "import": "./dist/core.mjs",
      "require": "./dist/core.js"
    },
    "./next": {
      "types": "./dist/next.d.ts",
      "import": "./dist/next.mjs",
      "require": "./dist/next.js"
    },
    "./sveltekit": {
      "types": "./dist/sveltekit.d.ts",
      "import": "./dist/sveltekit.mjs",
      "require": "./dist/sveltekit.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/core.ts src/react.tsx src/next.tsx src/astro.ts src/sveltekit.ts --format cjs,esm --dts"
  },
  "keywords": [
    "web-vitals",
    "performance",
    "pulsebear"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "web-vitals": "^5.1.0"
  }
}
